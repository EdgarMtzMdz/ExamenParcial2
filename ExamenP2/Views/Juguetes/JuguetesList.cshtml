@model IEnumerable<Juguetes>

@{
    ViewData["Title"] = "Lista de Juguetes";
}

<h1>Juguetes</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">
                Id
            </th>

        <th scope="col">
                Codigo
            </th>
            <th scope="col">
                Nombre
            </th>
            <th scope="col">
                Precio
            </th>
            <th scope="col">
                Vigencia
            </th>
            <th scope="col">
                Activo
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Juguetes in Model)
        {
            <tr>
                <td>
                    @Juguetes.idJuguetes
                </td>
                <td>
                    @Juguetes.Codigo
                </td>
                <td>
                    @Juguetes.Nombre
                </td>
                
                <td>
                    @Juguetes.Precio
                </td>
                <td>
                    @Juguetes.Vigencia
                </td>
                <td>
                   @Juguetes.Activo
                </td>
                @*<td>
                    <a class="btn btn-outline-info" asp-action="EmpleadosUpdate" asp-controller="Empleados" asp-route-IdEmpleados="@Empleados.IdEmpleados">
                        Editar
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-danger" asp-action="EmpleadosDelete" asp-controller="Empleados" asp-route-IdEmpleados="@Empleados.IdEmpleados">
                        Borrar
                    </a>
                </td>*@
            </tr>
        }
    </tbody>
</table>

<h2>
    <a class="btn btn-outline-info" asp-action="JuguetesAdd" asp-controller="Juguetes"
    >
        Agregar
    </a>
</h2>